    </div> --}}

    <div class="uk-width-1-4@s">
        <label class="uk-form-label" for="form-horizontal-select">Phone Number:</label>
        <input class="uk-input phoneInput" type="text" maxlength="3" id="areaCode" placeholder="909">
        <input class="uk-input phoneInput" type="text" maxlength="3" id="phonePrefix" placeholder="665">
        <input class="uk-input phoneInput" type="text" maxlength="4" id="phoneLineNumber" placeholder="8732">
    </div>

    <div class="uk-width-1-4@s">
        <div class="uk-margin">
            <label class="uk-form-label" for="form-horizontal-select">State:</label>
            <div class="uk-form-controls">
                <select class="uk-select" id="form-horizontal-select stateInput">
